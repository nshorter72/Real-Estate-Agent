{
  "name": "real-estate-agent",
  "version": "0.1.0",
  "description": "React component for post-offer management and timeline generation",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "build:watch": "tsup src/index.ts --watch --format cjs,esm --dts",
    "example:dev": "cd example && npm install && npm run dev"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "lucide-react": ">=0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nshorter72/Real-Estate-Agent.git"
  },
  "homepage": "https://nshorter72.github.io/Real-Estate-Agent",
  "devDependencies": {
    "typescript": "^5.2.2",
    "tsup": "^6.7.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7"
  },
  "keywords": [
    "react",
    "component",
    "real-estate",
    "timeline",
    "agent"
  ],
  "license": "MIT"
}
